{
  "id": "/finance1",
  "cmd": null,
  "cpus": 0.1,
  "mem": 128,
  "disk": 64,
  "instances": 2,
  "acceptedResourceRoles": [
    "*"
  ],
  "container": {
    "type": "DOCKER",
    "volumes": [],
    "docker": {
      "image": "vishaldenge/stocknew",
      "network": "BRIDGE",
      "portMappings": [
        {
          "containerPort": 8000,
          "hostPort": 0,
